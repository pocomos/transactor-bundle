{
  "name": "pocomos/transactor-bundle",
  "description": "Integrates the Orkestra Transactor with Symfony 2",
  "type": "symfony-bundle",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/pocomos/transactor"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "pocomos/transactor": "dev-master"
  },
  "require-dev": {
    "symfony/framework-bundle":  "~2.3",
    "symfony/dependency-injection":  "~2.3",
    "symfony/yaml": "~2.3",
    "symfony/form": "~2.3",
    "doctrine/orm": "~2.3"
  },
  "autoload": {
    "psr-0": { "Orkestra\\Bundle\\TransactorBundle": "" }
  },
  "target-dir": "Orkestra/Bundle/TransactorBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
